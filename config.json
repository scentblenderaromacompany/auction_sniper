{
    "auth_info": {
        "username": "your_username",
        "password": "your_password"
    },
    "bid_sniper": {
        "snipe_time": 30
    },
    "logging": {
        "version": 1,
        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "level": "DEBUG",
                "formatter": "simpleFormatter"
            }
        },
        "formatters": {
            "simpleFormatter": {
                "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
            }
        },
        "root": {
            "level": "DEBUG",
            "handlers": ["console"]
        }
    }
}
